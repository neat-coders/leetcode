class Solution {
    public int maxProfit(int[] prices) {
       int maxProfit = 0;
       int profit = 0;
       int i , j ;
       for(i=1 ; i<prices.length ; i++) {
        for(j=0 ; j<i ; j++){
            profit = prices[i] - prices[j];
            if(profit > maxProfit){
                maxProfit = profit;
            }
        }
       }
       return maxProfit;
    }
}
